	$('#requestAddress').html($('#infoAddress').val().toUpperCase());
	$('#requestPhone').html($('#infoPhone').val().toUpperCase());
	$('#requestEmail').html($('#infoEmail').val().toUpperCase());
	$('#requestMFG').html($('#infoMFG').val().toUpperCase());
	$('#requestModel').html($('#infoModel').val().toUpperCase());
	$('#requestSerial').html($('#infoSerial').val().toUpperCase());
	$('#requestDOP').html($('#infoDOP').val().toUpperCase());
	$('#requestInvoice').html($('#infoInvoice').val().toUpperCase());
	$.ajax({
	  type:"GET",
	  url:"get_dealer",
	  dataType:"json",
	  data: {submitted_code: $('#infoDealerCode').val()},
	  success:function(result){
	  	var fieldValue = $('#infoDealerCode').val();
	  	if (fieldValue.length == 0) {

	  	} else {
