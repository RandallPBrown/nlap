}


function emailSet(){
	$('#dealerSubject').val("REPLACEMENT QUOTE REQUEST: " + $('#infoCustomer').val().toUpperCase() + " - " + $('#infoSO').val().toUpperCase() + " - M: " + $('#infoModel').val().toUpperCase() + "/S: " + $('#infoSerial').val().toUpperCase());
