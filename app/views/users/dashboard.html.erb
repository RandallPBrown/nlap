<!-- COOKIE MODAL BELOW (FUNCTIONING). CAN BE USED FOR INSTRUCTIONS OR TO GIVE A WEEKLY BREAKDOWN -->
<script>
$(document).ready(function() {
    if (Cookies.get('morning_modal') == null) {
        Cookies.set('morning_modal', 'yes', { expires: 1, path: '/' });
        $('#morningModal').modal('show');
    }
    // highlight entry card
    if (<%= @user_entry_total_effective_monthly %> == 0.5) {
      $('#occurrence-card').addClass('bg-warning text-white');
      $('#occurrence-modal').addClass('bg-warning text-white');
    } else if (<%= @user_entry_total_effective_monthly %> > 0.5) {
      $('#occurrence-card').removeClass('float').addClass('muted');
      $('#occurrence-modal').addClass('bg-danger text-white')
    } else {
      $('#occurrence-card').removeClass('bg-warning text-white')
      $('#occurrence-modal').addClass('bg-success');
    }
    // highlight wu card
    if (<%= @user_writeup_written %> > 0) {
      $('#wu-card').removeClass('float').addClass('muted');
      $('#wu-modal').addClass('bg-danger text-white');
    } else {
      $('#wu-card').removeClass('bg-warning text-white')
      $('#wu-modal').addClass('bg-success');
    }
    // highlight uph card
    if (<%= number_with_precision(@uph, :precision => 1) %> < 6.5) {
      $('#uph-card').addClass('bg-warning text-white');
      $('#uph-modal').addClass('bg-warning text-white');
    } else {
      $('#uph-card').removeClass('bg-warning text-white');
      $('#uph-modal').addClass('bg-success');
    }
    // highlight occ card
    if (<%= number_with_precision(@occ, :precision => 1) %> < 98) {
      $('#occupancy-card').addClass('bg-warning text-white');
      $('#occupancy-modal').addClass('bg-warning text-white');
    } else {
      $('#occupancy-card').removeClass('bg-warning text-white');
      $('#occupancy-modal').addClass('bg-success');
    }
    // highlight err card
    if (<%= @err_logs_count %> > 0) {
      $('#error-card').removeClass('float').addClass('muted');
      $('#error-modal').addClass('bg-danger text-white');
    } else {
      $('#error-card').removeClass('bg-warning text-white');
      $('#error-modal').addClass('bg-success');
    }   
});


</script>
